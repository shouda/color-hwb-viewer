{
  "name": "color-hwb-viewer",
  "version": "0.0.1",
  "description": "HWB Color Space Viewer",
  "main": "''",
  "scripts": {
    "build": "NODE_ENV=development webpack --config webpack.config.js --profile --progress --colors",
    "deploy": "NODE_ENV=production webpack -p --config webpack.config.js --optimize-minimize; script/chunkhash.js",
    "dev": "NODE_ENV=development node --harmony_async_await server-koa.js",
    "pro": "NODE_ENV=production node --harmony_async_await server-koa.js",
    "test": "NODE_ENV=test ava",
    "test:lib": "NODE_ENV=test ava --source lib/*-test.js",
    "new-component": "plop component",
    "new-stateless": "plop stateless",
    "serve": "npm run deploy; npm run pro"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shouda/color-hwb-viewer"
  },
  "keywords": [
    "React",
    "Redux",
    "Babel",
    "Webpack",
    "ESlint",
    "Basscss"
  ],
  "author": "Wei Shou-Da",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shouda/color-hwb-viewer/issues"
  },
  "homepage": "https://github.com/shouda/color-hwb-viewer#readme",
  "engines": {
    "node": ">=7.0.0"
  },
  "dependencies": {
    "basscss": "~8.0.3",
    "basscss-background-colors": "~2.1.0",
    "basscss-btn": "~1.1.1",
    "basscss-btn-outline": "~1.1.0",
    "basscss-btn-primary": "~1.1.0",
    "basscss-btn-sizes": "~1.2.0",
    "color2": "~1.0.8",
    "immutable": "~3.8.1",
    "normalize.css": "~5.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "~4.4.6",
    "react-router": "~4.0.0-alpha.6",
    "redux": "~3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "~2.1.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "~6.16.0",
    "babel-register": "^6.18.0",
    "css-loader": "^0.26.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.8.3",
    "koa": "2.0.0-alpha.7",
    "koa-convert": "^1.2.0",
    "koa-static": "^3.0.0",
    "koa-webpack-dev-middleware": "^1.3.0",
    "koa-webpack-hot-middleware": "~1.0.3",
    "plop": "^1.7.2",
    "postcss": "^5.2.6",
    "postcss-browser-reporter": "~0.5.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "8.1.0",
    "postcss-loader": "^1.1.1",
    "postcss-reporter": "^2.0.0",
    "postcss-url": "~5.1.2",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "~0.13.1",
    "webpack": "2.1.0-beta.25"
  },
  "ava": {
    "babel": {
      "presets": [
        "react"
      ],
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "require": [
      "babel-register",
      "ignore-styles",
      "./test/helpers/setup.js"
    ]
  }
}
